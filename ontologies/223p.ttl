@prefix bacnet: <http://data.ashrae.org/bacnet/2020#> .
@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix bsh: <https://brickschema.org/schema/BrickShape#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix g36: <http://data.ashrae.org/standard223/1.0/extension/g36#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qkdv: <http://qudt.org/vocab/dimensionvector/> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix qudtqk: <http://qudt.org/vocab/quantitykind/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ref: <https://brickschema.org/schema/Brick/ref#> .
@prefix s223: <http://data.ashrae.org/standard223#> .
@prefix schema1: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix tag: <https://brickschema.org/schema/BrickTag#> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

s223:Class a rdfs:Class,
        sh:NodeShape ;
    rdfs:label "Class" ;
    rdfs:subClassOf rdfs:Class ;
    sh:property [ rdfs:comment "This Class must have a label" ;
            sh:path rdfs:label ;
            sh:sparql [ a sh:SPARQLConstraint ;
                    sh:message "{$this} must have an rdfs:label" ;
                    sh:prefixes s223: ;
                    sh:select """
SELECT $this
WHERE {
FILTER (NOT EXISTS {$this rdfs:label ?something}) .
}
""" ] ] .

g36:AirFlowStation a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "Air Flow Station" ;
    sh:class s223:Sensor ;
    sh:property [ rdfs:comment "An air flow station must have an airflow property that is measured by an airflow sensor." ;
            sh:path s223:hasProperty ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:QuantifiableObservableProperty ;
                    sh:node [ sh:property [ sh:hasValue qudtqk:VolumeFlowRate ;
                                    sh:maxCount 1 ;
                                    sh:minCount 1 ;
                                    sh:path qudt:hasQuantityKind ],
                                [ sh:class s223:Medium-Air ;
                                    sh:minCount 1 ;
                                    sh:path s223:measuresSubstance ] ] ] ;
            sh:severity s223:g36 ] .

g36:ChilledWaterCoil a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "ChilledWaterCoil" ;
    sh:class s223:CoolingCoil ;
    sh:property [ rdfs:comment "The ChilledWaterCoil must be connected to a ChilledWaterValve using the relation connected. " ;
            sh:path s223:connected ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:ChilledWaterValve ] ;
            sh:severity s223:g36 ] .

g36:ChilledWaterValve a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "Chilled Water Valve" ;
    sh:class s223:Valve ;
    sh:or ( [ sh:property [ rdfs:comment "A ChilledWaterValve must have at least one analog valve command property or at least two binary valve command property." ;
                        sh:path s223:hasProperty ;
                        sh:qualifiedMinCount 1 ;
                        sh:qualifiedValueShape [ sh:class s223:QuantifiableActuatableProperty ;
                                sh:node [ sh:property [ sh:hasValue qudtqk:DimensionlessRatio ;
                                                sh:maxCount 1 ;
                                                sh:minCount 1 ;
                                                sh:path qudt:hasQuantityKind ],
                                            [ sh:hasValue unit:PERCENT ;
                                                sh:maxCount 1 ;
                                                sh:minCount 1 ;
                                                sh:path qudt:hasUnit ] ] ] ;
                        sh:severity s223:g36 ] ] [ sh:property [ rdfs:comment "A ChilledWaterValve must have at least one analog valve command property or at least two binary valve command property." ;
                        sh:path s223:hasProperty ;
                        sh:qualifiedMinCount 2 ;
                        sh:qualifiedValueShape [ sh:class s223:EnumerableActuatableProperty ;
                                sh:node [ sh:property [ sh:maxCount 1 ;
                                                sh:minCount 1 ;
                                                sh:path s223:hasEnumerationKind ] ] ] ;
                        sh:severity s223:g36 ] ] ) ;
    sh:property [ rdfs:comment "A chilled water valve must hasMedium Water-ChilledWater." ;
            sh:hasValue s223:Water-ChilledWater ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path s223:hasMedium ;
            sh:severity s223:g36 ] .

g36:Damper a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "Damper" ;
    sh:class s223:Damper ;
    sh:or ( [ sh:property [ rdfs:comment "A damper must have at least one analog damper command property or two binary damper command property using the relation hasProperty." ;
                        sh:path s223:hasProperty ;
                        sh:qualifiedMinCount 1 ;
                        sh:qualifiedValueShape [ sh:class s223:QuantifiableActuatableProperty ;
                                sh:node [ sh:property [ sh:hasValue qudtqk:DimensionlessRatio ;
                                                sh:maxCount 1 ;
                                                sh:minCount 1 ;
                                                sh:path qudt:hasQuantityKind ],
                                            [ sh:hasValue unit:PERCENT ;
                                                sh:maxCount 1 ;
                                                sh:minCount 1 ;
                                                sh:path qudt:hasUnit ] ] ] ;
                        sh:severity s223:g36 ] ] [ sh:property [ rdfs:comment "A damper must have at least one analog damper command property or two binary damper command property using the relation hasProperty." ;
                        sh:path s223:hasProperty ;
                        sh:qualifiedMinCount 2 ;
                        sh:qualifiedValueShape [ sh:class s223:EnumerableActuatableProperty ;
                                sh:node [ sh:property [ sh:maxCount 1 ;
                                                sh:minCount 1 ;
                                                sh:path s223:hasEnumerationKind ] ] ] ;
                        sh:severity s223:g36 ] ] ) .

g36:DualDuctTerminalWithDischargeSensor a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "DualDuctTerminalWithDischargeSensor" ;
    sh:class s223:DualDuctTerminal ;
    sh:property [ rdfs:comment "DualDuctTerminalWithDischargeSensor shall contain 2 dampers" ;
            sh:path s223:contains ;
            sh:qualifiedMaxCount 2 ;
            sh:qualifiedMinCount 2 ;
            sh:qualifiedValueShape [ sh:class g36:Damper ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "In DualDuctTerminalWithDischargeSensor, one Damper has Role Heating" ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:Damper ;
                    sh:node [ sh:property [ sh:path s223:hasRole ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:hasValue s223:Role-Heating ] ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "In DualDuctTerminalWithDischargeSensor, one Damper has Role Cooling" ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:Damper ;
                    sh:node [ sh:property [ sh:path s223:hasRole ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:hasValue s223:Role-Cooling ] ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The DualDuctTerminalWithDischargeSensor OutletConnectionPoint or an OutletConnectionPoint that mapsTo the DualDuctTerminalWithDischargeSensor OutletConnectionPoint must have one discharge airflow property that is measured by a discharge airflow sensor. The discharge airflow sensor hasObservationLocation at an OutletConnectionPoint." ;
            sh:path s223:hasConnectionPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:OutletConnectionPoint ;
                    sh:node [ sh:property [ sh:path ( [ sh:zeroOrOnePath [ sh:inversePath s223:mapsTo ] ] s223:hasProperty ) ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:class s223:QuantifiableObservableProperty ;
                                            sh:node [ sh:property [ sh:hasValue qudtqk:VolumeFlowRate ;
                                                            sh:maxCount 1 ;
                                                            sh:minCount 1 ;
                                                            sh:path qudt:hasQuantityKind ] ] ] ] ] ] ;
            sh:severity s223:g36 ] .

g36:DualDuctTerminalWithInletSensors a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "DualDuctTerminalWithInletSensors" ;
    rdfs:comment "I wonder if there is better naming" ;
    sh:class s223:DualDuctTerminal ;
    sh:property [ rdfs:comment "DualDuctTerminalWithInletSensors shall contain 2 dampers" ;
            sh:class g36:Damper ;
            sh:maxCount 2 ;
            sh:minCount 2 ;
            sh:path s223:contains ;
            sh:severity s223:g36 ],
        [ rdfs:comment "DualDuctTerminalWithInletSensors shall contain 1 damper that has the Role Heating." ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:Damper ;
                    sh:node [ sh:property [ sh:path s223:hasAspect ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:hasValue s223:Role-Heating ] ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "DualDuctTerminalWithInletSensors shall contain 1 damper that has the Role Cooling." ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:Damper ;
                    sh:node [ sh:property [ sh:path s223:hasAspect ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:hasValue s223:Role-Cooling ] ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The DualDuctTerminalWithInletSensors InletConnectionPoint or an InletConnectionPoint that mapsTo the DualDuctTerminalWithInletSensors InletConnectionPoint must have a cooling supply airflow property that is measured by a cooling supply airflow sensor.  The cooling supply airflow sensor hasObservationLocation at an InletConnectionPoint." ;
            sh:path s223:hasConnectionPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:InletConnectionPoint ;
                    sh:node [ sh:property [ sh:path ( [ sh:zeroOrOnePath [ sh:inversePath s223:mapsTo ] ] s223:hasProperty ) ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:class s223:QuantifiableObservableProperty ;
                                            sh:node [ sh:property [ sh:hasValue qudtqk:VolumeFlowRate ;
                                                            sh:maxCount 1 ;
                                                            sh:minCount 1 ;
                                                            sh:path qudt:hasQuantityKind ],
                                                        [ sh:path s223:hasAspect ;
                                                            sh:qualifiedMinCount 1 ;
                                                            sh:qualifiedValueShape [ sh:hasValue s223:Role-Cooling ] ] ] ] ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The DualDuctTerminalWithInletSensors InletConnectionPoint or an InletConnectionPoint that mapsTo the DualDuctTerminalWithInletSensors InletConnectionPoint must have a heating supply airflow property that is measured by a heating supply airflow sensor.  The heating supply airflow sensor hasObservationLocation at an InletConnectionPoint." ;
            sh:path s223:hasConnectionPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:InletConnectionPoint ;
                    sh:node [ sh:property [ sh:path ( [ sh:zeroOrOnePath [ sh:inversePath s223:mapsTo ] ] s223:hasProperty ) ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:class s223:QuantifiableObservableProperty ;
                                            sh:node [ sh:property [ sh:hasValue qudtqk:VolumeFlowRate ;
                                                            sh:maxCount 1 ;
                                                            sh:minCount 1 ;
                                                            sh:path qudt:hasQuantityKind ],
                                                        [ sh:path s223:hasAspect ;
                                                            sh:qualifiedMinCount 1 ;
                                                            sh:qualifiedValueShape [ sh:hasValue s223:Role-Heating ] ] ] ] ] ] ] ;
            sh:severity s223:g36 ] .

g36:ElectricHeatingCoil a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "Electric Heating Coil" ;
    sh:class s223:ResistanceHeater ;
    sh:or ( [ sh:property [ rdfs:comment "An electric heating coil must have at least one analog heating command property or two binary heating command property." ;
                        sh:path s223:hasProperty ;
                        sh:qualifiedMinCount 1 ;
                        sh:qualifiedValueShape [ sh:class s223:QuantifiableActuatableProperty ;
                                sh:node [ sh:property [ sh:hasValue qudtqk:DimensionlessRatio ;
                                                sh:maxCount 1 ;
                                                sh:minCount 1 ;
                                                sh:path qudt:hasQuantityKind ],
                                            [ sh:hasValue unit:PERCENT ;
                                                sh:maxCount 1 ;
                                                sh:minCount 1 ;
                                                sh:path qudt:hasUnit ] ] ] ;
                        sh:severity s223:g36 ] ] [ sh:property [ rdfs:comment "An electric heating coil must have at least one analog heating command property or two binary heating command property." ;
                        sh:path s223:hasProperty ;
                        sh:qualifiedMinCount 2 ;
                        sh:qualifiedValueShape [ sh:class s223:EnumerableActuatableProperty ;
                                sh:node [ sh:property [ sh:maxCount 1 ;
                                                sh:minCount 1 ;
                                                sh:path s223:hasEnumerationKind ] ] ] ;
                        sh:severity s223:g36 ] ] ) .

g36:Fan a rdfs:Class,
        sh:NodeShape ;
    rdfs:label "Fan" ;
    sh:class s223:Fan ;
    sh:property [ rdfs:comment "A Fan must have at least one Start/Stop command." ;
            sh:path s223:hasProperty ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:EnumerableActuatableProperty ;
                    sh:node [ sh:property [ sh:hasValue s223:EnumerationKind-OnOff ;
                                    sh:path s223:hasEnumerationKind ] ] ] ;
            sh:severity s223:g36 ] .

g36:FanPoweredTerminal a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "FanPoweredTerminal" ;
    rdfs:comment "Has all the rules in VAVWithReheat, but we don't have multiple parent classes in s223." ;
    sh:class s223:FanPoweredTerminal ;
    sh:property [ rdfs:comment "A FanPoweredTerminal must have exactly one Damper." ;
            sh:path s223:contains ;
            sh:qualifiedMaxCount 1 ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:Damper ] ;
            sh:severity s223:g36 ],
        [ sh:path s223:contains ;
            sh:qualifiedMaxCount 1 ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:or ( [ sh:class g36:Fan ] [ sh:class g36:FanWithVFD ] ) ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The FanPoweredTerminal InletConnectionPoint or an InletConnectionPoint that mapsTo the FanPoweredTerminal InletConnectionPoint must have a supply airflow property that is measured by a supply airflow sensor.  The supply airflow sensor hasObservationLocation at an InletConnectionPoint." ;
            sh:path s223:hasConnectionPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:InletConnectionPoint ;
                    sh:node [ sh:property [ sh:path ( [ sh:zeroOrOnePath [ sh:inversePath s223:mapsTo ] ] s223:hasProperty ) ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:class s223:QuantifiableObservableProperty ;
                                            sh:node [ sh:property [ sh:hasValue qudtqk:VolumeFlowRate ;
                                                            sh:maxCount 1 ;
                                                            sh:minCount 1 ;
                                                            sh:path qudt:hasQuantityKind ] ] ] ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "A FanPoweredTerminal must contain a hot water coil or an electric heating coil for heating." ;
            sh:path s223:contains ;
            sh:qualifiedMaxCount 1 ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:or ( [ sh:class g36:HotWaterCoil ] [ sh:class g36:ElectricHeatingCoil ] ) ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The FanPoweredTerminal OutletConnectionPoint or the OutletConnectionPoint that mapsTo the FanPoweredTerminal OutletConnectionPoint must have a discharge air temperature property that is being measured by a discharge air temperature sensor. The discharge air temperature sensor hasObservationLocation at an OutletConnectionPoint. " ;
            sh:path s223:hasConnectionPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:OutletConnectionPoint ;
                    sh:node [ sh:property [ sh:path ( [ sh:zeroOrOnePath [ sh:inversePath s223:mapsTo ] ] s223:hasProperty ) ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:class s223:QuantifiableObservableProperty ;
                                            sh:node [ sh:property [ sh:hasValue qudtqk:Temperature ;
                                                            sh:maxCount 1 ;
                                                            sh:minCount 1 ;
                                                            sh:path qudt:hasQuantityKind ] ] ] ] ] ] ;
            sh:severity s223:g36 ] .

g36:FanWithVFD a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "FanWithVFD" ;
    sh:class s223:Fan ;
    sh:property [ rdfs:comment "A fan with VFD must have a fan start command. We may need a speed aspect. Is AnalogIn property with dimensionless quantitykind on a fan clear enough?" ;
            sh:path s223:hasProperty ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:EnumerableActuatableProperty ;
                    sh:node [ sh:property [ sh:hasValue s223:EnumerationKind-OnOff ;
                                    sh:maxCount 1 ;
                                    sh:minCount 1 ;
                                    sh:path s223:hasEnumerationKind ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "A fan with VFD must have a fan speed command" ;
            sh:path s223:hasProperty ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:QuantifiableActuatableProperty ;
                    sh:node [ sh:property [ sh:maxCount 1 ;
                                    sh:minCount 1 ;
                                    sh:path qudt:hasQuantityKind ;
                                    sh:value qudtqk:DimensionlessRatio ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "A fan with VFD must have a fan start status" ;
            sh:path s223:hasProperty ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:EnumerableObservableProperty ;
                    sh:node [ sh:property [ sh:hasValue s223:EnumerationKind-OnOff ;
                                    sh:maxCount 1 ;
                                    sh:minCount 1 ;
                                    sh:path s223:hasEnumerationKind ] ] ] ;
            sh:severity s223:g36 ] .

g36:Filter a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "Filter" ;
    sh:class s223:AirFilter ;
    sh:property [ rdfs:comment "AirFilter must have at least one differential pressure property. The differential pressure property is observed by a differential pressure sensor. The differential pressure sensor hasObservationLocationHigh at an InletConnectionPoint and hasObservationLocationHigh at an OutletConnectionPoint. " ;
            sh:path s223:hasProperty ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:QuantifiableObservableProperty ;
                    sh:node [ sh:property [ sh:hasValue qudtqk:Pressure ;
                                    sh:maxCount 1 ;
                                    sh:minCount 1 ;
                                    sh:path qudt:hasQuantityKind ],
                                [ sh:class s223:Sensor ;
                                    sh:node [ sh:property [ sh:class s223:InletConnectionPoint ;
                                                    sh:minCount 1 ;
                                                    sh:path s223:hasObservationLocationHigh ],
                                                [ sh:class s223:OutletConnectionPoint ;
                                                    sh:minCount 1 ;
                                                    sh:path s223:hasObservationLocationLow ] ] ;
                                    sh:path [ sh:inversePath s223:observes ] ] ] ] ;
            sh:severity s223:g36 ] .

g36:HotWaterCoil a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "HotWater/ReheatCoil" ;
    sh:class s223:HeatingCoil ;
    sh:property [ rdfs:comment "The HotWaterCoil must be connected to a HotWaterValve using the relation connected. " ;
            sh:path s223:connected ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:HotWaterValve ] ;
            sh:severity s223:g36 ] .

g36:HotWaterValve a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "Hot Water Valve" ;
    sh:class s223:Valve ;
    sh:or ( [ sh:property [ rdfs:comment "A HotWaterValve must have at least one analog valve command property or at least two binary valve command property." ;
                        sh:path s223:hasProperty ;
                        sh:qualifiedMinCount 1 ;
                        sh:qualifiedValueShape [ sh:class s223:QuantifiableActuatableProperty ;
                                sh:node [ sh:property [ sh:hasValue qudtqk:DimensionlessRatio ;
                                                sh:maxCount 1 ;
                                                sh:minCount 1 ;
                                                sh:path qudt:hasQuantityKind ],
                                            [ sh:hasValue unit:PERCENT ;
                                                sh:maxCount 1 ;
                                                sh:minCount 1 ;
                                                sh:path qudt:hasUnit ] ] ] ;
                        sh:severity s223:g36 ] ] [ sh:property [ rdfs:comment "A HotWaterValve must have at least one analog valve command property or at least two binary valve command property." ;
                        sh:path s223:hasProperty ;
                        sh:qualifiedMinCount 2 ;
                        sh:qualifiedValueShape [ sh:class s223:EnumerableActuatableProperty ;
                                sh:node [ sh:property [ sh:maxCount 1 ;
                                                sh:minCount 1 ;
                                                sh:path s223:hasEnumerationKind ] ] ] ;
                        sh:severity s223:g36 ] ] ) ;
    sh:property [ rdfs:comment "A hot water valve must hasMedium Water-HotWater." ;
            sh:hasValue s223:Water-HotWater ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path s223:hasMedium ;
            sh:severity s223:g36 ] .

g36:MultiZoneVAVAirHandlingUnit a rdfs:Class,
        sh:NodeShape ;
    sh:class s223:AirHandlingUnit ;
    sh:property [ rdfs:comment "The MultiZoneVAVAirHandlingUnit InletConnectionPoint or the InletConnectionPoint that mapsTo the MultiZoneVAVAirHandlingUnit InletConnectionPoint must have a return air temperature property that is being measured by a return air temperature sensor. The return air temperature sensor hasObservationLocation at an InletConnectionPoint. " ;
            sh:path s223:hasConnectionPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:InletConnectionPoint ;
                    sh:node [ sh:property [ sh:path ( [ sh:zeroOrOnePath [ sh:inversePath s223:mapsTo ] ] s223:hasProperty ) ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:class s223:QuantifiableObservableProperty ;
                                            sh:node [ sh:property [ sh:hasValue qudtqk:Temperature ;
                                                            sh:maxCount 1 ;
                                                            sh:minCount 1 ;
                                                            sh:path qudt:hasQuantityKind ],
                                                        [ sh:hasValue s223:Role-Return ;
                                                            sh:maxCount 1 ;
                                                            sh:minCount 1 ;
                                                            sh:path s223:hasAspect ] ] ] ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "MultiZoneVAVAirHandlingUnit's minimum outdoor air damper's OutletConnectionPoint has an outdoor air temperature property that is being measured by an outdoor air temperature sensor." ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:Damper ;
                    sh:node [ sh:property [ sh:hasValue s223:Role-Outside ;
                                    sh:maxCount 1 ;
                                    sh:minCount 1 ;
                                    sh:path s223:hasRole ],
                                [ sh:path s223:hasConnectionPoint ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:class s223:OutletConnectionPoint ;
                                            sh:node [ sh:property [ sh:path s223:hasProperty ;
                                                            sh:qualifiedMinCount 1 ;
                                                            sh:qualifiedValueShape [ sh:class s223:QuantifiableObservableProperty ;
                                                                    sh:node [ sh:property [ sh:hasValue qudtqk:Temperature ;
                                                                                    sh:maxCount 1 ;
                                                                                    sh:minCount 1 ;
                                                                                    sh:path qudt:hasQuantityKind ],
                                                                                [ sh:hasValue s223:Role-Outside ;
                                                                                    sh:maxCount 1 ;
                                                                                    sh:minCount 1 ;
                                                                                    sh:path s223:hasAspect ] ] ] ] ] ] ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The MultiZoneVAVAirHandlingUnit contains a Supply Fan with a VFD that has the role Supply" ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:FanWithVFD ;
                    sh:node [ sh:property [ sh:hasValue s223:Role-Supply ;
                                    sh:path s223:hasRole ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The MultiZoneVAVAirHandlingUnit contains an AirFilter and filter has property for pressure drop ." ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:Filter ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The MultiZoneVAVAirHandlingUnit OutletConnectionPoint or the OutletConnectionPoint that mapsTo the MultiZoneVAVAirHandlingUnit OutletConnectionPoint must have a supply air temperature property that is being measured by a supply air temperature sensor. The supply air temperature sensor hasObservationLocation at an OutletConnectionPoint. " ;
            sh:path s223:hasConnectionPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:OutletConnectionPoint ;
                    sh:node [ sh:property [ sh:path ( [ sh:zeroOrOnePath [ sh:inversePath s223:mapsTo ] ] s223:hasProperty ) ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:class s223:QuantifiableObservableProperty ;
                                            sh:node [ sh:property [ sh:hasValue qudtqk:Temperature ;
                                                            sh:maxCount 1 ;
                                                            sh:minCount 1 ;
                                                            sh:path qudt:hasQuantityKind ],
                                                        [ sh:hasValue s223:Role-Supply ;
                                                            sh:maxCount 1 ;
                                                            sh:minCount 1 ;
                                                            sh:path s223:hasAspect ] ] ] ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The MultiZoneVAVAirHandlingUnit OutletConnectionPoint or the OutletConnectionPoint that mapsTo the MultiZoneVAVAirHandlingUnit OutletConnectionPoint must have a supply duct static pressure property that is being measured by a supply duct static pressure sensor. The supply duct static pressure sensor hasObservationLocation at an OutletConnectionPoint. " ;
            sh:path s223:hasConnectionPoint ;
            sh:property [ sh:hasValue s223:Role-Supply ;
                    sh:maxCount 1 ;
                    sh:minCount 1 ;
                    sh:path s223:hasRole ] ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:OutletConnectionPoint ;
                    sh:node [ sh:property [ sh:path ( [ sh:zeroOrOnePath [ sh:inversePath s223:mapsTo ] ] s223:hasProperty ) ;
                                    sh:qualifiedMinCount 1 ;
                                    sh:qualifiedValueShape [ sh:class s223:QuantifiableObservableProperty ;
                                            sh:node [ sh:property [ sh:hasValue qudtqk:Pressure ;
                                                            sh:maxCount 1 ;
                                                            sh:minCount 1 ;
                                                            sh:path qudt:hasQuantityKind ] ] ] ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The MultiZoneVAVAirHandlingUnit must contain an economizer outdoor air damper." ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:Damper ;
                    sh:node [ sh:property [ sh:hasValue s223:Role-Economizer ;
                                    sh:maxCount 1 ;
                                    sh:minCount 1 ;
                                    sh:path s223:hasRole ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The MultiZoneVAVAirHandlingUnit must contain a return air damper." ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:Damper ;
                    sh:node [ sh:property [ sh:hasValue s223:Role-Return ;
                                    sh:maxCount 1 ;
                                    sh:minCount 1 ;
                                    sh:path s223:hasRole ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The  MultiZoneVAVAirHandlingUnit must contain at least one ChilledWaterCoil for cooling." ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:ChilledWaterCoil ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The  MultiZoneVAVAirHandlingUnit must contain at least one HotWaterCoil or an ElectricHeatingCoil for heating." ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:or ( [ sh:class g36:HotWaterCoil ] [ sh:class g36:ElectricHeatingCoil ] ) ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The  MultiZoneVAVAirHandlingUnit must contain an air flow station. The air flow station is used to measure the outdoor airflow property." ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:AirFlowStation ;
                    sh:node [ sh:property [ sh:hasValue s223:Role-Outside ;
                                    sh:minCount 1 ;
                                    sh:path s223:hasRole ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The  MultiZoneVAVAirHandlingUnit must contain a return fan. SPARQL rules would be easier, is anything lost through using them? Part of rules variant if a supply fan is included" ;
            sh:message "Return fan is not present in the MultiZoneVAVAirHandlingUnit." ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:FanWithVFD ;
                    sh:node [ sh:property [ sh:hasValue s223:Role-Return ;
                                    sh:minCount 1 ;
                                    sh:path s223:hasRole ] ] ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "The MultiZoneVAVAirHandlingUnit must contain an exhaust air damper." ;
            sh:path s223:contains ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:Damper ;
                    sh:node [ sh:property [ sh:hasValue s223:Role-Exhaust ;
                                    sh:maxCount 1 ;
                                    sh:minCount 1 ;
                                    sh:path s223:hasRole ] ] ] ;
            sh:severity s223:g36 ] .

g36:VAV a s223:Class,
        rdfs:Class,
        sh:NodeShape ;
    rdfs:label "VAV" ;
    sh:class s223:SingleDuctTerminal ;
    sh:property [ rdfs:comment "A VAV must contain exactly one Damper." ;
            sh:path s223:contains ;
            sh:qualifiedMaxCount 1 ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class g36:Damper ] ;
            sh:severity s223:g36 ],
        [ rdfs:comment "Not sure we want this in every rule, it may be better to just have a shacl rule we can apply to certain classes to put properties on the mapsTo for a given node.",
                "The VAV InletConnectionPoint or an InletConnectionPoint that mapsTo the VAV InletConnectionPoint must have one supply airflow property." ;
            sh:path s223:hasConnectionPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ sh:class s223:InletConnectionPoint ;
                    sh:node [ sh:property [ sh:path ( [ sh:zeroOrOnePath [ sh:inversePath s223:mapsTo ] ] s223:hasProperty ) ;
                                    sh:qualifiedMinCount 1 ;
