brick:Air_Handling_Unit a owl:Class,
        sh:NodeShape ;
    rdfs:label "Air Handling Unit" ;
    rdfs:subClassOf brick:HVAC_Equipment ;
    owl:equivalentClass brick:AHU,
        brick:Air_Handler_Unit ;
    sh:rule [ a sh:TripleRule ;
            sh:object tag:Air ;
            sh:predicate brick:hasTag ;
