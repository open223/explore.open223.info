                                                            sh:maxCount 1 ;
                                                            sh:minCount 1 ;
                                                            sh:path qudt:hasQuantityKind ] ] ] ] ] ] ;
            sh:severity s223:g36 ] .

g36:VAVWithReheat a s223:Class,
