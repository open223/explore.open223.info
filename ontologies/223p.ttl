            sh:object tag:Point ;
            sh:predicate brick:hasTag ;
            sh:subject sh:this ],
        [ a sh:TripleRule ;
            sh:object tag:Setpoint ;
            sh:predicate brick:hasTag ;
            sh:subject sh:this ] ;
    brick:hasAssociatedTag tag:Air,
        tag:Flow,
        tag:Limit,
        tag:Parameter,
        tag:Point,
        tag:Setpoint .

brick:Air_Grains_Sensor a owl:Class,
